; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GoZen Lite"
config/description="Very basic video player"
config/version="2024.06.05"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="gozen_lite"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/flush_stdout_on_print=true
config/icon="res://icon.svg"

[autoload]

Project="*res://autoloads/project.gd"
Settings="*res://autoloads/settings.gd"
InputHandler="*res://autoloads/input_handler.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2

[input]

project_save={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":true,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":83,"key_label":0,"unicode":115,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
